{
  "Basic: ti-image": {
    "prefix": "tiimg",
    "body": [
      "<ti-image src=\"${1}\" mode=\"${2|fill,cover,contain,none,scale-down,top,bottom,center,left,right,top left,top right,bottom left,bottom right,scaleToFill,aspectFit,aspectFill,widthFix,heightFix|}\" />"
    ],
    "description": "Titian <ti-image>图片</ti-image>"
  },
  "Basic: ti-icon": {
    "prefix": "tiicon",
    "body": ["<ti-icon name=\"${1}\" />"],
    "description": "Titian <ti-icon>图标</ti-icon>"
  },
  "Basic: ti-button": {
    "prefix": "tibutton",
    "body": ["<ti-button variant=\"${1|contained,filled,outlined,text|}\">${2}</ti-button>"],
    "description": "Titian <ti-button>按钮</ti-button>"
  },
  "Basic: ti-tag": {
    "prefix": "titag",
    "body": ["<ti-tag variant=\"${1|contained,filled,outlined|}\">${2}</ti-tag>"],
    "description": "Titian <ti-tag>标签</ti-tag>"
  },
  "Basic: ti-cell": {
    "prefix": "ticell",
    "body": ["<ti-cell title=\"${1}\">${2}</ti-cell>"],
    "description": "Titian <ti-cell>单元格</ti-cell>"
  },
  "Basic: ti-divider": {
    "prefix": "tidivider",
    "body": ["<ti-divider>${1}</ti-divider>"],
    "description": "Titian <ti-divider>分割线</ti-divider>"
  },
  "Layout: ti-layout": {
    "prefix": "tilayout",
    "body": [
      "<ti-row>",
      "\t<ti-col span=\"${1:12}\">${2}</ti-col>",
      "\t<ti-col span=\"${3:12}\">${4}</ti-col>",
      "</ti-row>"
    ],
    "description": "Titian <ti-layout>布局</ti-layout>"
  },
  "Layout: ti-sticky": {
    "prefix": "tisticky",
    "body": ["<ti-sticky>${1}</ti-sticky>"],
    "description": "Titian <ti-sticky>粘性布局</ti-sticky>"
  },
  "Layout: ti-grid": {
    "prefix": "tigrid",
    "body": [
      "<ti-grid>",
      "\t<ti-grid-item icon=\"${1}\" text=\"${2}\" />",
      "\t<ti-grid-item icon=\"${3}\" text=\"${4}\" />",
      "</ti-grid>"
    ],
    "description": "Titian <ti-grid>宫格</ti-grid>"
  },
  "Form: ti-input": {
    "prefix": "tiinput",
    "body": ["<ti-input value=\"${1}\" placeholder=\"${2}\" />"],
    "description": "Titian <ti-input>输入框</ti-input>"
  },
  "Form: ti-textarea": {
    "prefix": "titextarea",
    "body": ["<ti-textarea value=\"${1}\" placeholder=\"${2}\" />"],
    "description": "Titian <ti-textarea>文本域</ti-textarea>"
  },
  "Form: ti-switch": {
    "prefix": "tiswitch",
    "body": ["<ti-switch value=\"${1}\" />"],
    "description": "Titian <ti-switch>开关</ti-switch>"
  },
  "Form: ti-radio": {
    "prefix": "tiradio",
    "body": [
      "<ti-radio-group>",
      "\t<ti-radio label=\"${1}\" value=\"${2}\" />",
      "\t<ti-radio label=\"${3}\" value=\"${4}\" />",
      "</ti-radio-group>"
    ],
    "description": "Titian <ti-radio>单选框</ti-radio>"
  },
  "Form: ti-checkbox": {
    "prefix": "tiradio",
    "body": [
      "<ti-checkbox-group>",
      "\t<ti-checkbox label=\"${1}\" value=\"${2}\" />",
      "\t<ti-checkbox label=\"${3}\" value=\"${4}\" />",
      "</ti-checkbox-group>"
    ],
    "description": "Titian <ti-checkbox>复选框</ti-checkbox>"
  },
  "Form: ti-rate": {
    "prefix": "tirate",
    "body": ["<ti-rate value=\"${1}\" />"],
    "description": "Titian <ti-rate>评分</ti-rate>"
  },
  "Form: ti-slider": {
    "prefix": "tislider",
    "body": ["<ti-slider value=\"${1}\" />"],
    "description": "Titian <ti-slider>滑块</ti-slider>"
  },
  "Form: ti-input-number": {
    "prefix": "tiinput-number",
    "body": ["<ti-input-number value=\"${1}\" />"],
    "description": "Titian <ti-input-number>步进器</ti-input-number>"
  },
  "Form: ti-uploader": {
    "prefix": "tiuploader",
    "body": ["<ti-uploader bind:change=\"${1}\" />"],
    "description": "Titian <ti-uploader>文件上传</ti-uploader>"
  },
  "Form: ti-search": {
    "prefix": "tisearch",
    "body": ["<ti-search value=\"${1}\" placeholder=\"${2}\" bind:search=\"${3}\" />"],
    "description": "Titian <ti-search>搜索</ti-search>"
  },
  "Form: ti-picker": {
    "prefix": "tipicker",
    "body": ["<ti-picker value=\"${1}\" option=\"${2}\" bind:change=\"${3}\" />"],
    "description": "Titian <ti-picker>选择器</ti-picker>"
  },
  "Form: ti-datetime-picker": {
    "prefix": "tidatetime-picker",
    "body": ["<ti-datetime-picker value=\"${1}\" bind:confirm=\"${2}\" />"],
    "description": "Titian <ti-datetime-picker>时间选择</ti-datetime-picker>"
  },
  "Form: ti-calendar": {
    "prefix": "ticalendar",
    "body": ["<ti-calendar value=\"${1}\" bind:confirm=\"${2}\" />"],
    "description": "Titian <ti-calendar>日历</ti-calendar>"
  },
  "Display: ti-countdown": {
    "prefix": "ticountdown",
    "body": ["<ti-countdown time=\"${1}\" />"],
    "description": "Titian <ti-countdown>倒计时</ti-countdown>"
  },
  "Display: ti-progress": {
    "prefix": "tiprogress",
    "body": ["<ti-progress value=\"${1}\" />"],
    "description": "Titian <ti-progress>进度条</ti-progress>"
  },
  "Display: ti-empty": {
    "prefix": "tiempty",
    "body": ["<ti-empty title=\"${1}\">${2}</ti-empty>"],
    "description": "Titian <ti-empty>空态</ti-empty>"
  },
  "Display: ti-steps": {
    "prefix": "tisteps",
    "body": ["<ti-steps option=\"${1}\" />"],
    "description": "Titian <ti-steps>步骤条</ti-steps>"
  },
  "Display: ti-badge": {
    "prefix": "tibadge",
    "body": ["<ti-badge content=\"${1}\" />"],
    "description": "Titian <ti-badge>徽标</ti-badge>"
  },
  "Display: ti-notice-bar": {
    "prefix": "tinotice-bar",
    "body": ["<ti-notice-bar content=\"${1}\" />"],
    "description": "Titian <ti-notice-bar>通告栏</ti-notice-bar>"
  },
  "Action: ti-toast": {
    "prefix": "titoast",
    "body": ["<ti-toast id=\"titian-toast\" />"],
    "description": "Titian <ti-toast>轻提示</ti-toast>"
  },
  "Action: ti-tooltip": {
    "prefix": "titooltip",
    "body": ["<ti-tooltip content=\"${1}\">${2}</ti-tooltip>"],
    "description": "Titian <ti-tooltip>文字提示</ti-tooltip>"
  },
  "Action: ti-popup": {
    "prefix": "tipopup",
    "body": ["<ti-popup visible=\"${1}\">${2}</ti-popup>"],
    "description": "Titian <ti-popup>弹出层</ti-popup>"
  },
  "Action: ti-popup-titlebar": {
    "prefix": "tipopup-titlebar",
    "body": ["<ti-popup-titlebar title=\"${1}\" bind:confirm=\"${2}\" />"],
    "description": "Titian <ti-popup-titlebar>弹出层标题</ti-popup-titlebar>"
  },
  "Action: ti-dialog": {
    "prefix": "tidialog",
    "body": ["<ti-dialog id=\"titian-dialog\" />"],
    "description": "Titian <ti-dialog>弹出框</ti-dialog>"
  },
  "Action: ti-action-sheet": {
    "prefix": "tiaction-sheet",
    "body": ["<ti-action-sheet visible=\"${1}\" actions=\"${2}\" bind:select=\"${3}\" />"],
    "description": "Titian <ti-action-sheet>动作面板</ti-action-sheet>"
  },
  "Action: ti-share-sheet": {
    "prefix": "tishare-sheet",
    "body": ["<ti-share-sheet visible=\"${1}\" actions=\"${2}\" />"],
    "description": "Titian <ti-share-sheet>分享面板</ti-share-sheet>"
  },
  "Action: ti-collapse": {
    "prefix": "ticollapse",
    "body": ["<ti-collapse>", "\t<ti-collapse-item title=\"${1}\">${2}</ti-collapse-item>", "</ti-collapse>"],
    "description": "Titian <ti-collapse>折叠面板</ti-collapse>"
  },
  "Action: ti-dropdown-menu": {
    "prefix": "tidropdown-menu",
    "body": [
      "<ti-dropdown-menu>",
      "\t<ti-dropdown-item options=\"${1}\" bind:change=\"${2}\">${3}</ti-dropdown-item>",
      "</ti-dropdown-menu>"
    ],
    "description": "Titian <ti-dropdown-menu>下拉菜单</ti-dropdown-menu>"
  },
  "Action: ti-loading": {
    "prefix": "tiloading",
    "body": ["<ti-loading />"],
    "description": "Titian <ti-loading>加载</ti-loading>"
  },
  "Action: ti-back-top": {
    "prefix": "tiback-top",
    "body": ["<ti-back-top />"],
    "description": "Titian <ti-back-top>返回顶部</ti-back-top>"
  },
  "Action: ti-swipe-cell": {
    "prefix": "tiswipe-cell",
    "body": ["<ti-swipe-cell>${1}</ti-swipe-cell>"],
    "description": "Titian <ti-swipe-cell>滑动单元格</ti-swipe-cell>"
  },
  "Navigation: ti-sidebar": {
    "prefix": "tisidebar",
    "body": ["<ti-sidebar bind:change=\"${1}\">", "\t<ti-sidebar-item label=\"${2}\" />", "</ti-sidebar>"],
    "description": "Titian <ti-sidebar>侧边导航</ti-sidebar>"
  },
  "Navigation: ti-tree-select": {
    "prefix": "titree-select",
    "body": ["<ti-tree-select option=\"${1}\">${2}</ti-tree-select>"],
    "description": "Titian <ti-tree-select>分类选择</ti-tree-select>"
  },
  "Navigation: ti-tabs": {
    "prefix": "titabs",
    "body": ["<ti-tabs tabs=\"${1}\" bind:change=\"${2}\">${3}</ti-tabs>"],
    "description": "Titian <ti-tabs>标签页</ti-tabs>"
  },
  "Navigation: ti-tabbar": {
    "prefix": "titabbar",
    "body": ["<ti-tabbar bind:select=\"${1}\">", "\t<ti-tabbar-item title=\"${2}\" icon=\"${3}\" />", "</ti-tabbar>"],
    "description": "Titian <ti-tabbar>侧边导航</ti-tabbar>"
  }
}
