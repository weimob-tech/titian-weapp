.titian-back-top{align-items:center;background:var(--back-top-bg-color,var(--neutral-color-9,#fff));border:1px solid var(--neutral-color-6,#f2f2f2);border-radius:var(--back-top-border-radius,50%);bottom:var(--back-top-bottom,200rpx);box-shadow:var(--back-top-shadow,0 6rpx 20rpx 0 rgba(0,0,0,.06));display:flex;flex-direction:column;height:var(--back-top-width,96rpx);justify-content:center;position:fixed;right:var(--back-top-right,28rpx);width:var(--back-top-width,96rpx);z-index:var(--back-top-zindex,10)}.titian-back-top-text{font-size:var(--back-top-text-font-size,20rpx)}@keyframes fade{0%{opacity:0}to{opacity:0}}@keyframes fade-in{0%{opacity:0}to{opacity:1}}@keyframes fade-out{0%{opacity:1}to{opacity:0}}@keyframes slide-in-up{0%{opacity:0;transform:translate3d(0,172rpx,0)}to{transform:translateZ(0)}}@keyframes slide-out-up{0%{transform:translateZ(0)}to{transform:translate3d(0,172rpx,0)}}.titian-slide-up-enter-active{animation:fade .1s cubic-bezier(.48,.33,.24,.95),fade-in .1s cubic-bezier(.48,.33,.24,.95) .1s,slide-in-up .2s cubic-bezier(.48,.33,.24,.95);animation-fill-mode:forwards}.titian-slide-up-exit-active{animation:fade-out .1s cubic-bezier(.48,.33,.24,.95) .1s,slide-out-up .2s cubic-bezier(.48,.33,.24,.95);animation-fill-mode:forwards}