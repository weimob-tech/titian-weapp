.titian-rate,.titian-rate-item{align-items:center;display:inline-flex;justify-content:center}.titian-rate-item{margin-right:var(--rate-gap,24rpx);position:relative}.titian-rate-item:last-of-type{margin-right:0}.titian-rate-star-half{bottom:0;left:0;margin:auto;overflow:hidden;position:absolute!important;top:0;width:50%}.titian-rate-filled{--icon-color:var(--rate-color,rgb(var(--theme-r,250),var(--theme-g,44),var(--theme-b,25)))}.titian-rate-empty{--icon-color:var(--rate-empty-color,var(--neutral-color-5,#e0e0e0))}