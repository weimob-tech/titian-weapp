:host{display:contents}.titian-image{display:inline-block;overflow:hidden;position:relative;vertical-align:bottom;width:100%;z-index:0}.titian-image-ratio:after{content:"";display:block;padding-bottom:calc(1/(var(--image-aspect-ratio, 1))*100%)}@supports (aspect-ratio:1){.titian-image-ratio{aspect-ratio:var(--image-aspect-ratio,1)}.titian-image-ratio:after{content:none}}.titian-image-target{display:block;height:100%;width:100%}.titian-image-target-ratio{left:0;position:absolute;top:0}.titian-image-target-hidden{opacity:0}.titian-image-notice{--loading-color:var(--neutral-color-5,#e0e0e0);color:var(--neutral-color-5,#e0e0e0);font-size:28rpx;left:50%;position:absolute;top:50%;transform:translate(-50%,-50%);white-space:nowrap;z-index:-1}