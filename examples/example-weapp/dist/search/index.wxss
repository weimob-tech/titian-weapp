:host{flex:1}.titian-search{background:#fff;font-size:28rpx;height:108rpx;overflow:hidden}.titian-search,.titian-search-input-container{align-items:center;display:flex;padding:0 28rpx}.titian-search-input-container{background:var(--neutral-color-7,#f5f5f5);border-radius:var(--search-radius,calc(var(--capsule-radius-size, 0px) + 12rpx));flex:1;height:72rpx}.titian-search-input-host{flex:1}.titian-search-virtual-placeholder{align-items:center;display:flex;height:100%;justify-content:center;width:100%}.titian-search-placeholder-text{color:var(--neutral-color-3,#9e9e9e)}.titian-search-virtualtext{color:var(--neutral-color-1,#212121)}.titian-search-button-box{overflow:hidden;transform:translateZ(0);transition:width .2s ease-in;width:0}.titian-search-button-box-focus{overflow:visible;width:84rpx}.titian-search-button-box-pure{transition:none}.titian-search .search-icon{--icon-color:var(--search-icon-color,var(--neutral-color-1,#212121));line-height:normal;margin-right:16rpx}.titian-search .search-input{background-color:transparent!important;padding:0!important;width:100%}.titian-search .search-button{padding-right:0!important}.titian-search-suffix-icon{align-items:center;display:flex;margin-left:15rpx}