.page {
  box-sizing: border-box;
  height: 100vh;
  overflow: hidden;

  background-color: #fff;
}

.navigation-bar {
  display: flex;
  position: fixed;
  z-index: 10;
  top: 0;
  right: 0;
  align-items: center;
  justify-content: center;
  width: 100vw;

  border-bottom: 1rpx solid #f2f2f2;

  opacity: 0;
  background: #fff;

  color: #191919;
  font-size: 34rpx;
  font-weight: 500;
}

.root-bg {
  width: 100vw;

  vertical-align: middle;
}

.group {
  --neutral-color-1: #9e9e9e;
  --cell-group-title-padding-h: 24rpx;
  --cell-group-title-padding-v: 16rpx;
  --cell-group-margin-h: 48rpx;
  --cell-background-color: #f9f9f9;
  --cell-padding-v: 32rpx;
  --cell-padding-h: 24rpx;
  --cell-right-icon-color: #c4c4c4;
  --cell-bg-color: #f9f9f9;
}

.group-title {
  color: #9e9e9e !important;
  font-size: 24rpx !important;
  font-weight: 400 !important;
  line-height: 28rpx !important;
}

.title-wrap {
  margin-top: 60rpx;
  margin-bottom: 12rpx;

  --cell-group-title-padding-v: 16rpx;
  --cell-group-title-padding-h: 48rpx;
  --cell-group-margin-h: 48rpx;
  --cell-bg-color: #f9f9f9;
  --cell-padding-v: 32rpx;
  --cell-padding-h: 24rpx;
}

.cell {
  --neutral-color-1: #212121;
  --cell-label-text-color: #c4c4c4;
  height: 128rpx;
  margin-bottom: 32rpx;

  border-radius: 12rpx;
}

.cell-last {
  margin-bottom: 0;
}

.cell-title {
  font-weight: 600 !important;
  line-height: 34rpx !important;
}

.cell-labal {
  font-size: 22rpx !important;
  line-height: 26rpx !important;
}

.cell::after {
  display: none;
}

.cell::before {
  content: '';

  position: absolute;
  top: 0;
  left: 0;
  box-sizing: border-box;
  width: 200%;
  height: 200%;

  transform: scale(0.5);
  transform-origin: left top;

  border: 2rpx solid #f2f2f2;
  border-radius: 24rpx;
}

.group .titian-cell-group-title {
  color: #9e9e9e;
  font-weight: 400;
  line-height: 1;
}

.group .titian-cell-group-title-wrap {
  margin-top: 60rpx;
  margin-bottom: 12rpx;
}

.group .titian-cell-title {
  font-weight: 600;
  line-height: 34rpx;
}

.group .titian-cell-label {
  margin-top: 4rpx;

  font-size: 22rpx;
  line-height: 26rpx;
}

.group:last-child {
  margin-bottom: 32rpx;
}
