.container {
  min-height: 100vh;

  background: #f5f5f5;
}

.prefix,
.suffix {
  display: flex;
  align-items: center;
  justify-content: space-between;

  color: #111;
  font-size: 28rpx;
  font-weight: 400;
}

.prefix {
  margin-right: 28rpx;
}

.suffix {
  margin-left: 28rpx;
}

.search-box {
  border-radius: 20rpx;

  --icon-color: #212121;
}

.search-box-diy {
  background: #f3f3f3;
}

.diy {
  background-color: #f3f3f3;
}

.search-inner-class {
  border-radius: 72rpx;

  background-color: #fff;
}
